{
  "compilerOptions": {
    "target": "ES2020",        
    "module": "CommonJS",      
    "rootDir": "src",			//important main file should be inside src
    "outDir": "dist",          		//dist is an folder created by ts to place generated js file
    "strict": false,           
    "esModuleInterop": true,   
    "skipLibCheck": true
  }
}