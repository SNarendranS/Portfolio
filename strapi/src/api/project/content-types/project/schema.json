{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "projectName": {
      "type": "string",
      "default": "name",
      "required": true
    },
    "gitURL": {
      "type": "string"
    },
    "liveURL": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "demoScreenshots": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "demoVideo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files",
        "videos"
      ]
    },
    "projectDescription": {
      "type": "text",
      "default": "description",
      "required": true
    },
    "techStack": {
      "type": "json",
      "required": true
    }
  }
}
